{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
  "allowJs": true,
  "checkJs": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": ["node", "express", "jsonwebtoken"],
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.js"]
}
